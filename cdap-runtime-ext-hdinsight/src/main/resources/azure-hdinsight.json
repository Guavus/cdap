{
  "icon": {
    "type": "inline",
    "arguments": {
      "data": "data:image/png;base64,"
    }
  },
  "configuration-groups": [
    {
      "label": "Azure Account Information",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Subscription ID",
          "name": "subscriptionId",
          "required": true,
          "description": "Azure Subscription ID",
          "widget-attributes": {
            "placeholder": "Subscription ID"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Tenant ID",
          "name": "tenantId",
          "required": true,
          "description": "Tenant ID for your Azure Subscription",
          "widget-attributes": {
            "placeholder": "Tenant ID"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Client ID",
          "name": "clientId",
          "required": true,
          "description": "Service Principal Client ID used to make call to azure management API",
          "widget-attributes": {
            "placeholder": "Client ID"
          }
        },
        {
          "widget-type": "securekey-textarea",
          "label": "Client Secret",
          "name": "clientSecret",
          "required": true,
          "description": "Service Principal Client Secret used to make call to azure management API",
          "widget-attributes": {
            "placeholder": "Client Secret"
          }
        }
      ]
    },
    {
      "label": "Cluster Settings",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Resource Group Name",
          "name": "resourceGroupName",
          "required": true,
          "description": "A resource group is a container that holds related resources for an Azure solution. The resource group includes those resources that you want to manage as a group.",
          "widget-attributes": {
            "placeholder": "Specify your Resource Group Name"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Storage Account Name",
          "name": "storageAccountName",
          "required": true,
          "description": "An Azure storage account contains all of your Azure Storage data objects: blobs, files, queues, tables, and disks.",
          "widget-attributes": {
            "placeholder": "Specify your Storage Account Name"
          }
        },
        {
          "widget-type": "securekey-textarea",
          "label": "Storage Account Key",
          "name": "storageAccountKey",
          "required": true,
          "description": "Key used to authorize access to your storage account via Shared Key.",
          "widget-attributes": {
            "placeholder": "Specify your Storage Account Key"
          }
        },
        {
          "widget-type": "securekey-textarea",
          "label": "Cluster Password",
          "name": "clusterPassword",
          "required": true,
          "description": "Password used to access HDInsight ambari UI and to log on the vm with the cdap user. The password must be at least 10 characters in length and must contain at least one digit, one uppercase and one lower case letter, one non-alphanumeric character (except characters ' \" ` \\).,",
          "widget-attributes": {
            "placeholder": "Specify your Cluster Password"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Cluster Name prefix",
          "name": "clusterNamePrefix",
          "required": false,
          "description": "A prefix to use for all cluster name created with this profile. Choose something unique. Must start with a letter and can only contains letter, number and hyphen. Length of the prefix must be between 4 and 30 characters.",
          "widget-attributes": {
            "placeholder": "Specify your Cluster Name Prefix",
            "default": "cdap-"
          }
        },
        {
          "widget-type": "select",
          "label": "Location",
          "name": "location",
          "description": "Location where to launch the HDInsight cluster. For more information, refer to https://azure.microsoft.com/en-ca/global-infrastructure/locations/.",
          "widget-attributes": {
            "values_sourced_from": "az account list-locations -o json | jq '.[].displayName'",
            "values": [
              "East Asia",
              "Southeast Asia",
              "Central US",
              "East US",
              "East US 2",
              "West US",
              "North Central US",
              "South Central US",
              "North Europe",
              "West Europe",
              "Japan West",
              "Japan East",
              "Brazil South",
              "Australia East",
              "Australia Southeast",
              "South India",
              "Central India",
              "West India",
              "Canada Central",
              "Canada East",
              "UK South",
              "UK West",
              "West Central US",
              "West US 2",
              "Korea Central",
              "Korea South",
              "France Central",
              "France South",
              "Australia Central",
              "Australia Central 2",
              "UAE Central",
              "UAE North",
              "South Africa North",
              "South Africa West",
              "Switzerland North",
              "Switzerland West",
              "Germany North",
              "Germany West Central"
            ],
            "size": "medium",
            "default": "East US 2"
          }
        }
      ]
    },
    {
      "label": "Nodes Setting",
      "properties": [
        {
          "widget-type": "number",
          "label": "Worker Node Count",
          "name": "workerNodeCount",
          "description": "Number of worker nodes to deploy in the cluster",
          "widget-attributes": {
            "placeholder": "Specify your Cluster Password",
            "default": 2,
            "min": 2,
            "size": "small"
          }
        },
        {
          "widget-type": "select",
          "label": "Head nodes vm size",
          "name": "headNodeVmSize",
          "description": "",
          "widget-attributes": {
            "values": [
              "Standard_D12_v2",
              "Standard_D13_v2",
              "Standard_D14_v2",
              "Standard_E2_v3",
              "Standard_E4_v3",
              "Standard_E8_v3",
              "Standard_E16_v3",
              "Standard_E20_v3",
              "Standard_E32_v3",
              "Standard_E48_v3",
              "Standard_E64_v3"

            ],
            "size": "medium",
            "default": "Standard_D12_v2"
          }
        },
        {
          "widget-type": "select",
          "label": "Worker nodes vm size",
          "name": "workerNodeVmSize",
          "description": "",
          "widget-attributes": {
            "values": [
              "Standard_D12_v2",
              "Standard_D13_v2",
              "Standard_D14_v2",
              "Standard_E4_v3",
              "Standard_E8_v3",
              "Standard_E16_v3",
              "Standard_E20_v3",
              "Standard_E32_v3",
              "Standard_E48_v3",
              "Standard_E64_v3"
            ],
            "size": "medium",
            "default": "Standard_D13_v2"
          }
        }
      ]
    }
  ]
}
